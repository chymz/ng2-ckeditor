    "@types/core-js": "^2.5.0",
    "@types/node": "^10.11.0",
    "prettier": "^1.14.3",
    "rxjs": "^6.3.2",
    "tslint": "^5.11.0",
    "typescript": "3.0.3",
    "zone.js": "^0.8.26"
  },
